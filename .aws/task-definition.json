{
  "family": "natchan-portfolio-task-definition-1",
  "containerDefinitions": [
      {
          "name": "natchan-portfolio-image-2",
          "image": "802743044846.dkr.ecr.ap-northeast-1.amazonaws.com/natchan-portfolio",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "natchan-portfolio-image-2-80-tcp",
                  "containerPort": 80,
                  "hostPort": 80,
                  "protocol": "tcp",
                  "appProtocol": "http"
              },
              {
                  "name": "natchan-portfolio-image-2-443-tcp",
                  "containerPort": 443,
                  "hostPort": 443,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "environment": [],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "ulimits": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-create-group": "true",
                  "awslogs-group": "/ecs/natchan-portfolio-task-definition-1",
                  "awslogs-region": "ap-northeast-1",
                  "awslogs-stream-prefix": "ecs"
              }
          },
          "healthCheck": {
              "command": [
                  "CMD-SHELL",
                  "netstat -tulpn | grep :80"
              ],
              "interval": 30,
              "timeout": 5,
              "retries": 3
          }
      }
  ],
  "taskRoleArn": "arn:aws:iam::802743044846:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::802743044846:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}